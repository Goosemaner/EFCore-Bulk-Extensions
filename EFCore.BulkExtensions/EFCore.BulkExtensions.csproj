<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>2.0.5</Version>
    <Authors>borisdj</Authors>
    <Title>EFCore.BulkExtensions</Title>
    <Description>EntityFrameworkCore Bulk Extensions for Insert Update and Delete (CUD) operations on SQL Server.</Description>
    <PackageProjectUrl>https://github.com/borisdj/EFCore.BulkExtensions</PackageProjectUrl>
    <PackageIconUrl>http://i.imgur.com/k5tvW1x.png</PackageIconUrl>
    <Company />
    <PackageLicenseUrl>https://github.com/borisdj/EFCore.BulkExtensions/blob/master/LICENSE</PackageLicenseUrl>
    <PackageTags>EntityFrameworkCore Entity Framework Core EFCore EF Bulk Extensions Insert Update Delete</PackageTags>
    <PackageReleaseNotes>Added support for specifying exact columns to update.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FastMember.Signed" Version="1.1.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="2.0.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="2.0.0" />
    <PackageReference Include="System.Data.SqlClient" Version="4.4.0" />
  </ItemGroup>

</Project>